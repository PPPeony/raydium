{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "sourceMap": true,
    "baseUrl": "./",
    "strict": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": false,
    "paths": {
      "@/*": ["src/*"],
      "@@/*": ["src/.umi/*"],
      "@umijs/max": ["./node_modules/umi"],
      "@umijs/max/typings": ["src/.umi/typings"]
    }
  },
  "include": [
    "./typings.d.ts",
    "./src/.umi/typings.d.ts",
    "./src/**/*.ts",
    "./src/**/*.d.ts",
    "./src/**/*.tsx",
    "./*.ts",
    "./*.tsx"
  ],
  "exclude": ["node_modules"]
}
